{
    "languageserver": {
        "phplang": {
           "command": "php",
           "args": ["vendor/felixfbecker/language-server/bin/php-language-server.php"],
           "filetypes": ["php"]
        },
        "bash": {
           "command": "bash-language-server",
           "filetypes": ["sh"],
           "args": ["start"]
        },
        "elmLS": {
          "command": "elm-language-server",
          "filetypes": ["elm"],
          "rootPatterns": ["elm.json"],
          "initializationOptions": {
            "elmAnalyseTrigger": "change"
          }
        },
        "flow": {
          "command": "flow",
          "args": ["lsp"],
          "filetypes": ["javascript", "javascriptreact"],
          "initializationOptions": {},
          "requireRootPattern": true,
          "settings": {},
          "rootPatterns": [".flowconfig"]
        }
        
    },
    "diagnostic.displayByAle": true,
    "tsserver.log":"normal",
    "javascript.implementationsCodeLens.enable": true,
    "suggest.noselect": false,
    "suggest.floatEnable": true,
    "suggest.enablePreview" : true,
    "codeLens.enable": true,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.showUnused": true,
    "coc.preferences.enableFloatHighlight": true,
    "tsserver.implicitProjectConfig.checkJs": true,
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "javascript.suggest.autoImports": true

}

